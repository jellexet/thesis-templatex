@INPROCEEDINGS{home-scen1,
  author = {Jethro Tu and Wen-Wei Lin and Jung-Chih Wang and Yu-Tse Lin},
  title = {The Scenario Implementation of Home Networking},
  booktitle = {The 9th International Conference on Advanced Communication Technology},
  year = {2007},
  volume = {3},
  pages = {1861--1863},
  month = {February},
  abstract = {With the trend of digitizing household appliances, home networking
	becomes a popular topic. Today, it is an important issue to integrate
	the heterogeneous networking standard such as Ethernet network and
	power line network into one. To find out the problems and solutions,
	we take LonWorks, X10, and UPnP as an example of home network environments,
	and use OSGi service platform to integrate those different networks.
	We use the MHP and URC technology to gain the interaction with those
	devices. MHP could show information on TV and URC could be a common
	access control platform to solve too many remote controllers problem
	due to too many devices. Finally, we proposed some possible scenarios
	in home life and discuss how to perform them.},
  doi = {10.1109/ICACT.2007.358733},
  issn = {1738-9445},
  keywords = {domestic appliances, home computing, local area networksEthernet network,
	LonWorks, MHP, OSGi service platform, UPnP, URC technology, X10,
	access control platform, heterogeneous networking standard, home
	networking, household appliances, power line network, remote controllers
	problem, scenario implementation}
}

