@INPROCEEDINGS{byfl02,
  author = {Cena, G. and Valenzano, A.},
  title = {Performance analysis of Byteflight networks},
  booktitle = {Proceedings of the IEEE International Workshop on Factory Communication
	Systems},
  year = {2004},
  pages = {157--166},
  month = {September},
  abstract = {Byteflight is a high performance communication network conceived for
	the use in passenger vehicles, which features the advantages of both
	the synchronous and asynchronous transmission schemes. Its development
	was driven by the new needs of automotive systems, which could not
	be satisfied by the existing solutions. In this paper, the main features
	of Byteflight are analysed - pointing out its advantages and drawbacks
	- and its performance is evaluated for the different kinds of communications
	that have to be supported in today's vehicles as well as in advanced
	automated manufacturing systems.},
  doi = {10.1109/WFCS.2004.1377701},
  keywords = { controller area networks, performance evaluation, protocols, vehicles
	Byteflight networks, asynchronous transmission, automated manufacturing
	systems, automotive systems, high performance communication network,
	passenger vehicles, performance analysis, synchronous transmission}
}

